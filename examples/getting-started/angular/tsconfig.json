{
  "compilerOptions": {
    "strict": true,
    "skipLibCheck": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "moduleResolution": "bundler",
    "target": "ES2022",
    "module": "ES2022"
  },
  "angularCompilerOptions": {
    "strictTemplates": true
  },
  "files": ["src/main.ts"],
  "include": ["src/**/*.ts"]
}
